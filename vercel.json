{
  "version": 2,
  "framework": "remix",
  "buildCommand": "remix build",
  "devCommand": "remix dev",
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/build/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "has": [
        {
          "type": "header",
          "key": "x-middleware-next",
          "value": "1"
        }
      ],
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
